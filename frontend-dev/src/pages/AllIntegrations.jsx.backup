/* eslint-disable react/no-unstable-nested-components */
/* eslint-disable no-unused-expressions */
/* eslint-disable max-len */
import { lazy, memo, useCallback, useEffect, useState } from 'react'
import toast from 'react-hot-toast'
import { Link } from 'react-router-dom'
import Loader from '../components/Loaders/Loader'
import ConfirmModal from '../components/Utilities/ConfirmModal'
import MenuBtn from '../components/Utilities/MenuBtn'
import SingleToggle2 from '../components/Utilities/SingleToggle2'
import SnackMsg from '../components/Utilities/SnackMsg'
import Table from '../components/Utilities/Table'
import useFetch from '../hooks/useFetch'
import bitsFetch from '../Utils/bitsFetch'
import { __ } from '../Utils/i18nwrap'
import { useRecoilState } from 'recoil'
import { $flowStep, $newFlow } from '../GlobalStates'

const Welcome = lazy(() => import('./Welcome'))

// Local Storage Keys
const TAGS_STORAGE_KEY = 'bit_integration_tags'
const INTEGRATION_TAGS_STORAGE_KEY = 'bit_integration_tags_mapping'

// Tag Management Utilities
const getTagsFromStorage = () => {
  try {
    const tags = localStorage.getItem(TAGS_STORAGE_KEY)
    return tags ? JSON.parse(tags) : []
  } catch (e) {
    return []
  }
}

const saveTagsToStorage = tags => {
  try {
    localStorage.setItem(TAGS_STORAGE_KEY, JSON.stringify(tags))
  } catch (e) {
    console.error('Failed to save tags:', e)
  }
}

const getIntegrationTagsFromStorage = () => {
  try {
    const mapping = localStorage.getItem(INTEGRATION_TAGS_STORAGE_KEY)
    return mapping ? JSON.parse(mapping) : {}
  } catch (e) {
    return {}
  }
}

const saveIntegrationTagsToStorage = mapping => {
  try {
    localStorage.setItem(INTEGRATION_TAGS_STORAGE_KEY, JSON.stringify(mapping))
  } catch (e) {
    console.error('Failed to save integration tags:', e)
  }
}

function AllIntegrations({ isValidUser }) {
  const { data, isLoading, mutate } = useFetch({ payload: {}, action: 'flow/list', method: 'get' })
  const [integrations, setIntegrations] = useState(
    !isLoading && data.success && data?.data?.integrations ? data.data.integrations : []
  )
  const [snack, setSnackbar] = useState({ show: false })
  const [confMdl, setconfMdl] = useState({ show: false, btnTxt: '' })

  const [newFlow, setNewFlow] = useRecoilState($newFlow)
  const [flowStep, setFlowStep] = useRecoilState($flowStep)

  // Tag Management State
  const [tags, setTags] = useState(getTagsFromStorage())
  const [integrationTags, setIntegrationTags] = useState(getIntegrationTagsFromStorage())
  const [selectedTags, setSelectedTags] = useState([])
  const [showTagModal, setShowTagModal] = useState(false)
  const [newTagName, setNewTagName] = useState('')
  const [editingIntegrationId, setEditingIntegrationId] = useState(null)
  const [showTagAssignment, setShowTagAssignment] = useState(false)
  const [hoveredTagId, setHoveredTagId] = useState(null)

  useEffect(() => {
    setFlowStep(1)
    setNewFlow({})
  }, [])

  const [cols, setCols] = useState([
    {
      width: 250,
      minWidth: 80,
      Header: __('Trigger', 'bit-integrations'),
      accessor: 'triggered_entity'
    },
    { width: 250, minWidth: 80, Header: __('Action Name', 'bit-integrations'), accessor: 'name' },
    {
      width: 200,
      minWidth: 150,
      Header: __('Tags', 'bit-integrations'),
      accessor: 'tags',
      Cell: value => {
        const intTags = integrationTags[value.row.original.id] || []
        return (
          <div style={{ display: 'flex', flexWrap: 'wrap', gap: '4px', alignItems: 'center' }}>
            {intTags.map(tagId => {
              const tag = tags.find(t => t.id === tagId)
              return tag ? (
                <span
                  key={tagId}
                  style={{
                    padding: '4px 10px',
                    borderRadius: '14px',
                    fontSize: '11px',
                    backgroundColor: tag.color + '15',
                    color: tag.color,
                    border: `2px solid ${tag.color}`,
                    whiteSpace: 'nowrap',
                    fontWeight: '700',
                    boxShadow: `0 2px 4px ${tag.color}20`
                  }}>
                  {tag.name}
                </span>
              ) : null
            })}
            <button
              onClick={() => {
                setEditingIntegrationId(value.row.original.id)
                setShowTagAssignment(true)
              }}
              style={{
                padding: '4px 8px',
                fontSize: '13px',
                cursor: 'pointer',
                border: '2px dashed #cbd5e0',
                borderRadius: '8px',
                background: 'white',
                color: '#718096',
                fontWeight: '700',
                transition: 'all 0.2s'
              }}
              onMouseEnter={e => {
                e.target.style.borderColor = '#6f42c1'
                e.target.style.color = '#6f42c1'
                e.target.style.backgroundColor = '#6f42c110'
              }}
              onMouseLeave={e => {
                e.target.style.borderColor = '#cbd5e0'
                e.target.style.color = '#718096'
                e.target.style.backgroundColor = 'white'
              }}
              title={__('Manage tags', 'bit-integrations')}>
              +
            </button>
          </div>
        )
      }
    },
    {
      width: 200,
      minWidth: 200,
      Header: __('Created At', 'bit-integrations'),
      accessor: 'created_at'
    },
    // { width: 150, minWidth: 130, Header: __('Authorization Status', 'bit-integrations'), accessor: 'isAuthorized', Cell: value => <Button className="flx" action={(e) => handleStatus(e, value.row.original.id)} checked={Number(value.row.original.status) === 1} > Not Authorized <RemoveIcn size="14" className="icn-rotate-45" /></Button>  },
    {
      width: 70,
      minWidth: 60,
      Header: __('Status', 'bit-integrations'),
      accessor: 'status',
      Cell: value => (
        <SingleToggle2
          className="flx"
          action={e => handleStatus(e, value.row.original.id)}
          checked={Number(value.row.original.status) === 1}
        />
      )
    }
  ])

  useEffect(() => {
    !isLoading && setIntegrations(data.success ? data.data.integrations : [])
  }, [data])

  useEffect(() => {
    const ncols = cols.filter(itm => itm.accessor !== 't_action' && itm.accessor !== 'status')
    ncols.push({
      width: 70,
      minWidth: 60,
      Header: __('Status', 'bit-integrations'),
      accessor: 'status',
      Cell: value => (
        <SingleToggle2
          className="flx"
          action={e => handleStatus(e, value.row.original.id)}
          checked={Number(value.row.original.status) === 1}
        />
      )
    })
    ncols.push({
      sticky: 'right',
      width: 100,
      minWidth: 60,
      Header: __('Actions', 'bit-integrations'),
      accessor: 't_action',
      Cell: val => (
        <MenuBtn
          isValidUser={isValidUser}
          id={val.row.original.id}
          name={val.row.original.name}
          index={val.row.id}
          del={() => showDelModal(val.row.original.id, val.row.index)}
          dup={() => showDupMdl(val.row.original.id, val.row.index)}
        />
      )
    })
    setCols([...ncols])
  }, [integrations, tags, integrationTags])

  const handleStatus = (e, id) => {
    const status = e.target.checked
    const tmp = [...integrations]
    const integ = tmp.find(int => int.id === id)
    integ.status = status === true ? '1' : '0'
    setIntegrations(tmp)

    const param = { id, status }
    bitsFetch(param, 'flow/toggleStatus')
      .then(res => {
        toast.success(__(res.data, 'bit-integrations'))
      })
      .catch(() => {
        toast.error(__('Something went wrong', 'bit-integrations'))
      })
  }

  const handleDelete = (id, index) => {
    const tmpIntegrations = [...integrations]
    const deleteLoad = bitsFetch({ id }, 'flow/delete').then(response => {
      if (response.success) {
        tmpIntegrations.splice(index, 1)
        mutate(tmpIntegrations)
        setIntegrations(tmpIntegrations)
        return __('Integration deleted successfully', 'bit-integrations')
      }
      return response.data
    })

    toast.promise(deleteLoad, {
      success: msg => msg,
      error: __('Error Occurred', 'bit-integrations'),
      loading: __('delete...')
    })
  }

  const handleClone = id => {
    const loadClone = bitsFetch({ id }, 'flow/clone').then(response => {
      if (response.success) {
        const newInteg = response.data
        const tmpIntegrations = [...integrations]
        const exist = tmpIntegrations.find(item => item.id === id)
        const cpyInteg = {
          id: newInteg.id,
          name: `duplicate of ${exist.name}`,
          triggered_entity: exist.triggered_entity,
          status: exist.status,
          created_at: newInteg.created_at
        }
        tmpIntegrations.push(cpyInteg)
        setIntegrations(tmpIntegrations)
        return __('Integration clone successfully', 'bit-integrations')
      }
      return response.data
    })

    toast.promise(loadClone, {
      success: msg => msg,
      error: __('Error Occurred', 'bit-integrations'),
      loading: __('cloning...')
    })
  }

  const setBulkDelete = useCallback(
    rows => {
      const rowID = []
      const flowID = []
      for (let i = 0; i < rows.length; i += 1) {
        rowID.push(rows[i].id)
        flowID.push(rows[i].original.id)
      }
      const bulkDeleteLoading = bitsFetch({ flowID }, 'flow/bulk-delete').then(response => {
        if (response.success) {
          const newData = [...integrations]
          for (let i = rowID.length - 1; i >= 0; i -= 1) {
            newData.splice(Number(rowID[i]), 1)
          }
          setIntegrations(newData)
          return __('Integration Deleted Successfully', 'bit-integrations')
        }
        return response.data
      })

      toast.promise(bulkDeleteLoading, {
        success: msg => msg,
        error: __('Error Occurred', 'bit-integrations'),
        loading: __('delete...')
      })
      // eslint-disable-next-line react-hooks/exhaustive-deps
    },
    [integrations]
  )

  const setTableCols = useCallback(newCols => {
    setCols(newCols)
  }, [])
  const closeConfMdl = () => {
    confMdl.show = false
    setconfMdl({ ...confMdl })
  }
  const showDelModal = (id, index) => {
    confMdl.action = () => {
      handleDelete(id, index)
      closeConfMdl()
    }
    confMdl.btnTxt = __('Delete', 'bit-integrations')
    confMdl.btn2Txt = null
    confMdl.btnClass = ''
    confMdl.body = __('Are you sure to delete this Integration?', 'bit-integrations')
    confMdl.show = true
    setconfMdl({ ...confMdl })
  }

  const showDupMdl = formID => {
    confMdl.action = () => {
      handleClone(formID)
      closeConfMdl()
    }
    confMdl.btnTxt = __('Clone', 'bit-integration')
    confMdl.btn2Txt = null
    confMdl.btnClass = 'purple'
    confMdl.body = __('Are you sure to clone this Integration ?', 'bitform')
    confMdl.show = true
    setconfMdl({ ...confMdl })
  }

  // Tag Management Functions
  const addNewTag = () => {
    if (!newTagName.trim()) {
      toast.error(__('Please enter a tag name', 'bit-integrations'))
      return
    }

    if (tags.some(tag => tag.name.toLowerCase() === newTagName.toLowerCase())) {
      toast.error(__('Tag already exists', 'bit-integrations'))
      return
    }

    const newTag = {
      id: Date.now().toString(),
      name: newTagName.trim(),
      color: `#${Math.floor(Math.random() * 16777215).toString(16)}`
    }

    const updatedTags = [...tags, newTag]
    setTags(updatedTags)
    saveTagsToStorage(updatedTags)
    setNewTagName('')
    toast.success(__('Tag created successfully', 'bit-integrations'))
  }

  const deleteTag = tagId => {
    const updatedTags = tags.filter(tag => tag.id !== tagId)
    setTags(updatedTags)
    saveTagsToStorage(updatedTags)

    // Remove tag from all integrations
    const updatedMapping = { ...integrationTags }
    Object.keys(updatedMapping).forEach(intId => {
      updatedMapping[intId] = updatedMapping[intId].filter(tId => tId !== tagId)
    })
    setIntegrationTags(updatedMapping)
    saveIntegrationTagsToStorage(updatedMapping)

    // Remove from selected tags if selected
    setSelectedTags(selectedTags.filter(id => id !== tagId))
    toast.success(__('Tag deleted successfully', 'bit-integrations'))
  }

  const toggleTagForIntegration = (integrationId, tagId) => {
    const updatedMapping = { ...integrationTags }
    const currentTags = updatedMapping[integrationId] || []

    if (currentTags.includes(tagId)) {
      updatedMapping[integrationId] = currentTags.filter(id => id !== tagId)
    } else {
      updatedMapping[integrationId] = [...currentTags, tagId]
    }

    setIntegrationTags(updatedMapping)
    saveIntegrationTagsToStorage(updatedMapping)
  }

  const toggleTagFilter = tagId => {
    if (tagId === 'ALL') {
      setSelectedTags([])
      return
    }

    if (selectedTags.includes(tagId)) {
      setSelectedTags(selectedTags.filter(id => id !== tagId))
    } else {
      setSelectedTags([...selectedTags, tagId])
    }
  }

  const clearTagFilters = () => {
    setSelectedTags([])
  }

  // Filter integrations based on selected tags
  const filteredIntegrations =
    selectedTags.length > 0
      ? integrations.filter(integration => {
          const intTags = integrationTags[integration.id] || []
          return selectedTags.some(tagId => intTags.includes(tagId))
        })
      : integrations

  const loaderStyle = {
    display: 'flex',
    height: '82vh',
    justifyContent: 'center',
    alignItems: 'center'
  }

  if (isLoading) {
    return <Loader style={loaderStyle} />
  }

  return (
    <div id="all-forms">
      <style>{`
        @keyframes fadeIn {
          from { opacity: 0; }
          to { opacity: 1; }
        }
        @keyframes slideIn {
          from { 
            opacity: 0;
            transform: translateY(-20px) scale(0.95);
          }
          to { 
            opacity: 1;
            transform: translateY(0) scale(1);
          }
        }
      `}</style>
      <SnackMsg snack={snack} setSnackbar={setSnackbar} />
      <ConfirmModal
        show={confMdl.show}
        body={confMdl.body}
        action={confMdl.action}
        close={closeConfMdl}
        btnTxt={confMdl.btnTxt}
        btn2Txt={confMdl.btn2Txt}
        btn2Action={confMdl.btn2Action}
        btnClass={confMdl.btnClass}
      />

      {/* Create Tag Modal */}
      {showTagModal && (
        <div className="tag-modal-overlay" onClick={() => setShowTagModal(false)}>
          <div className="tag-modal-content" onClick={e => e.stopPropagation()}>
            <h3 className="tag-modal-title">{__('Create New Tag', 'bit-integrations')}</h3>

            <div style={{ marginBottom: '20px' }}>
              <label className="tag-modal-label">{__('Tag Name', 'bit-integrations')}</label>
              <input
                type="text"
                value={newTagName}
                onChange={e => setNewTagName(e.target.value)}
                onKeyPress={e => {
                  if (e.key === 'Enter') {
                    addNewTag()
                    setShowTagModal(false)
                  }
                }}
                placeholder={__('Enter tag name...', 'bit-integrations')}
                autoFocus
                className="tag-modal-input"
              />
            </div>

            <div className="tag-modal-actions">
              <button
                onClick={() => {
                  setShowTagModal(false)
                  setNewTagName('')
                }}
                className="tag-modal-btn-cancel">
                {__('Cancel', 'bit-integrations')}
              </button>
              <button
                onClick={() => {
                  addNewTag()
                  setShowTagModal(false)
                }}
                className="tag-modal-btn-create">
                {__('Create Tag', 'bit-integrations')}
              </button>
            </div>
          </div>
        </div>
      )}

      {/* Tag Assignment Modal */}
      {showTagAssignment && editingIntegrationId && (
        <div className="tag-modal-overlay">
          <div className="tag-assign-modal-content">
            <h3 className="tag-modal-title">{__('Assign Tags', 'bit-integrations')}</h3>

            {tags.length === 0 ? (
              <p className="tag-assign-empty">
                {__('No tags available. Please create tags first.', 'bit-integrations')}
              </p>
            ) : (
              <div className="tag-assign-list">
                {tags.map(tag => {
                  const isAssigned = (integrationTags[editingIntegrationId] || []).includes(tag.id)
                  return (
                    <label key={tag.id} className={`tag-assign-item ${isAssigned ? 'assigned' : ''}`}>
                      <input
                        type="checkbox"
                        checked={isAssigned}
                        onChange={() => toggleTagForIntegration(editingIntegrationId, tag.id)}
                      />
                      <span className="tag-assign-badge">{tag.name}</span>
                    </label>
                  )
                })}
              </div>
            )}

            <div className="tag-modal-actions">
              <button
                onClick={() => {
                  setShowTagAssignment(false)
                  setEditingIntegrationId(null)
                }}
                className="tag-modal-btn-done">
                {__('Done', 'bit-integrations')}
              </button>
            </div>
          </div>
        </div>
      )}

      {integrations && integrations?.length ? (
        <>
          <div className="af-header flx flx-between">
            <h2>{__('Integrations', 'bit-integrations')}</h2>

            <Link to="/flow/new" className="btn round btcd-btn-lg purple purple-sh">
              {__('Create Integration', 'bit-integrations')}
            </Link>
          </div>

          {/* Tag Filter Section */}
          <div className="tag-filter-container">
            <div className="tag-filter-header">
              <h4
                style={{
                  margin: 0,
                  fontSize: '16px',
                  fontWeight: '700',
                  color: '#1a1a1a',
                  letterSpacing: '-0.02em'
                }}>
                {__('Filter by Tags', 'bit-integrations')}
              </h4>
              {selectedTags.length > 0 && (
                <button
                  onClick={clearTagFilters}
                  style={{
                    padding: '6px 16px',
                    fontSize: '13px',
                    backgroundColor: '#ef4444',
                    color: 'white',
                    border: 'none',
                    borderRadius: '8px',
                    cursor: 'pointer',
                    fontWeight: '600',
                    transition: 'all 0.2s',
                    boxShadow: '0 2px 8px rgba(239, 68, 68, 0.3)'
                  }}
                  onMouseEnter={e => {
                    e.target.style.backgroundColor = '#dc2626'
                    e.target.style.transform = 'scale(1.05)'
                  }}
                  onMouseLeave={e => {
                    e.target.style.backgroundColor = '#ef4444'
                    e.target.style.transform = 'scale(1)'
                  }}>
                  {__('Clear Filters', 'bit-integrations')}
                </button>
              )}
            </div>
            <div style={{ display: 'flex', flexWrap: 'wrap', gap: '8px' }}>
              {/* ALL Tag - Default */}
              <button
                onClick={() => toggleTagFilter('ALL')}
                style={{
                  padding: '8px 18px',
                  borderRadius: '20px',
                  fontSize: '14px',
                  backgroundColor: selectedTags.length === 0 ? '#6f42c1' : 'white',
                  color: selectedTags.length === 0 ? 'white' : '#6f42c1',
                  border: '2px solid #6f42c1',
                  cursor: 'pointer',
                  fontWeight: '700',
                  transition: 'all 0.3s cubic-bezier(0.4, 0, 0.2, 1)',
                  boxShadow:
                    selectedTags.length === 0
                      ? '0 4px 12px rgba(111, 66, 193, 0.4)'
                      : '0 2px 6px rgba(0,0,0,0.1)',
                  textTransform: 'uppercase',
                  letterSpacing: '0.05em'
                }}
                onMouseEnter={e => {
                  e.target.style.transform = 'translateY(-2px)'
                  e.target.style.boxShadow = '0 6px 16px rgba(111, 66, 193, 0.4)'
                }}
                onMouseLeave={e => {
                  e.target.style.transform = 'translateY(0)'
                  e.target.style.boxShadow =
                    selectedTags.length === 0
                      ? '0 4px 12px rgba(111, 66, 193, 0.4)'
                      : '0 2px 6px rgba(0,0,0,0.1)'
                }}>
                {__('ALL', 'bit-integrations')}
                {selectedTags.length === 0 && ' ✓'}
              </button>

              {/* User-created tags with delete button on hover */}
              {tags.map(tag => {
                const isSelected = selectedTags.includes(tag.id)
                const isHovered = hoveredTagId === tag.id
                return (
                  <div
                    key={tag.id}
                    style={{
                      position: 'relative',
                      display: 'inline-flex',
                      alignItems: 'center'
                    }}
                    onMouseEnter={() => setHoveredTagId(tag.id)}
                    onMouseLeave={() => setHoveredTagId(null)}>
                    <button
                      onClick={() => toggleTagFilter(tag.id)}
                      style={{
                        padding: isHovered ? '8px 32px 8px 18px' : '8px 18px',
                        borderRadius: '20px',
                        fontSize: '14px',
                        backgroundColor: isSelected ? tag.color : 'white',
                        color: isSelected ? 'white' : tag.color,
                        border: `2px solid ${tag.color}`,
                        cursor: 'pointer',
                        fontWeight: '700',
                        transition: 'all 0.3s cubic-bezier(0.4, 0, 0.2, 1)',
                        boxShadow: isSelected ? `0 4px 12px ${tag.color}40` : '0 2px 6px rgba(0,0,0,0.1)'
                      }}
                      onMouseEnter={e => {
                        e.target.style.transform = 'translateY(-2px)'
                        e.target.style.boxShadow = `0 6px 16px ${tag.color}50`
                      }}
                      onMouseLeave={e => {
                        e.target.style.transform = 'translateY(0)'
                        e.target.style.boxShadow = isSelected
                          ? `0 4px 12px ${tag.color}40`
                          : '0 2px 6px rgba(0,0,0,0.1)'
                      }}>
                      {tag.name}
                      {isSelected && ' ✓'}
                    </button>
                    {isHovered && (
                      <button
                        onClick={e => {
                          e.stopPropagation()
                          deleteTag(tag.id)
                        }}
                        style={{
                          position: 'absolute',
                          right: '6px',
                          width: '20px',
                          height: '20px',
                          borderRadius: '50%',
                          backgroundColor: '#ef4444',
                          color: 'white',
                          border: 'none',
                          cursor: 'pointer',
                          fontSize: '14px',
                          display: 'flex',
                          alignItems: 'center',
                          justifyContent: 'center',
                          fontWeight: 'bold',
                          transition: 'all 0.2s',
                          zIndex: 10
                        }}
                        onMouseEnter={e => {
                          e.target.style.backgroundColor = '#dc2626'
                          e.target.style.transform = 'scale(1.15)'
                        }}
                        onMouseLeave={e => {
                          e.target.style.backgroundColor = '#ef4444'
                          e.target.style.transform = 'scale(1)'
                        }}
                        title={__('Delete tag', 'bit-integrations')}>
                        ×
                      </button>
                    )}
                  </div>
                )
              })}

              {/* Add Tag Button */}
              <button
                onClick={() => setShowTagModal(true)}
                style={{
                  padding: '8px 18px',
                  borderRadius: '20px',
                  fontSize: '16px',
                  backgroundColor: 'white',
                  color: '#10b981',
                  border: '2px dashed #10b981',
                  cursor: 'pointer',
                  fontWeight: '700',
                  transition: 'all 0.3s cubic-bezier(0.4, 0, 0.2, 1)',
                  boxShadow: '0 2px 6px rgba(0,0,0,0.1)',
                  display: 'inline-flex',
                  alignItems: 'center',
                  justifyContent: 'center',
                  minWidth: '40px'
                }}
                onMouseEnter={e => {
                  e.target.style.backgroundColor = '#10b981'
                  e.target.style.color = 'white'
                  e.target.style.transform = 'translateY(-2px)'
                  e.target.style.boxShadow = '0 6px 16px rgba(16, 185, 129, 0.4)'
                }}
                onMouseLeave={e => {
                  e.target.style.backgroundColor = 'white'
                  e.target.style.color = '#10b981'
                  e.target.style.transform = 'translateY(0)'
                  e.target.style.boxShadow = '0 2px 6px rgba(0,0,0,0.1)'
                }}
                title={__('Create new tag', 'bit-integrations')}>
                +
              </button>
            </div>
            {selectedTags.length > 0 && (
              <p
                style={{
                  margin: '16px 0 0 0',
                  fontSize: '14px',
                  color: '#64748b',
                  fontWeight: '600',
                  backgroundColor: '#f1f5f9',
                  padding: '8px 16px',
                  borderRadius: '10px',
                  display: 'inline-block'
                }}>
                {__('Showing', 'bit-integrations')}{' '}
                <span style={{ color: '#6f42c1', fontWeight: '700' }}>
                  {filteredIntegrations.length}
                </span>{' '}
                {__('of', 'bit-integrations')}{' '}
                <span style={{ fontWeight: '700' }}>{integrations.length}</span>{' '}
                {__('integrations', 'bit-integrations')}
              </p>
            )}
          </div>

          <div className="forms">
            <Table
              className="f-table btcd-all-frm"
              height={500}
              columns={cols}
              data={filteredIntegrations}
              rowSeletable
              resizable
              columnHidable
              setTableCols={setTableCols}
              setBulkDelete={setBulkDelete}
              search
            />
          </div>
        </>
      ) : (
        <Welcome isValidUser={isValidUser} integrations={integrations} />
      )}
    </div>
  )
}

export default memo(AllIntegrations)
